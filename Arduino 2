#include <SoftwareSerial.h>
#include <LiquidCrystal.h>
#include <string.h>

int rs = 12, en = 11 , db4 = 5, db5 = 4, db6 = 3, db7 = 2;
LiquidCrystal lcd(rs, en, db4, db5, db6, db7);

String cs;

void setup(){
	lcd.begin(16,2);
  	Serial.begin(9600);
}

void loop(){
	cs = Serial.readString();
  	Serial.flush();
  	lcd.print(cs);
  	delay(1000);
}
